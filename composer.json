{
    "name": "zerotoprod/mgid",
    "description": "An expressive and fluent package that interfaces with the MGID REST api.",
    "keywords": [
        "zerotoprod",
        "mgid"
    ],
    "homepage": "https://github.com/zerotoprod/mgid",
    "license": "MIT",
    "authors": [
        {
            "name": "David Smith",
            "email": "dasmith@zero-to-prod.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "guzzlehttp/guzzle": "^7.2",
        "ext-json": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.17",
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "Zerotoprod\\Mgid\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zerotoprod\\Mgid\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
